{
  "name": "gemini-cli-elasticsearch",
  "version": "0.1.0",
  "mcpServers": {
    "elastic-agent-builder": {
      "command": "npx",
      "args": [
        "-y",
        "mcp-remote",
        "${ELASTIC_MCP_URL}",
        "--header",
        "Authorization:${AUTH}"
      ],
      "env": {
        "AUTH": "ApiKey ${ELASTIC_API_KEY}"
      }
    }
  }
}
